{
  "name": "sportizen",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "build:prod-optimization": "ng build --prod --aot --buildOptimizer --commonChunk --vendorChunk --optimization --progress",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint the-class-master --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "pretty": "prettier --write \"./**/*.{html,css,scss,ts,js,jsx,json}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.6",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~11.0.6",
    "@angular/compiler": "~11.0.6",
    "@angular/core": "~11.0.6",
    "@angular/forms": "~11.0.6",
    "@angular/material": "^11.2.1",
    "@angular/platform-browser": "~11.0.6",
    "@angular/platform-browser-dynamic": "~11.0.6",
    "@angular/router": "~11.0.6",
    "@angular/service-worker": "~11.0.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@thisissoon/angular-inviewport": "^4.2.2",
    "@types/crypto-js": "^3.1.47",
    "@types/jquery": "^3.5.5",
    "angular-font-awesome": "^3.1.2",
    "angularx-social-login": "^2.3.1",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.3",
    "bson": "^4.2.2",
    "compression": "^1.7.4",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "hamburgers": "^1.1.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "ngx-image-cropper": "^3.3.2",
    "ngx-moment": "^5.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "waypoints": "^4.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "~11.0.6",
    "@angular/language-service": "~9.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "browser": {
    "crypto": false
  }
}
